    height: 41px;
  }
  .banner .content-box .content .logo img {
    width: 94px;
    height: 41px;
  }
  .banner .content-box .content .language {
    width: 64px;
    height: 19px;
    margin-top: 47px;
    border-radius: 3px;
    font-size: 12px;
  }
  .banner .content-box .content .language .icon {
    height: 7px;
    width: 10px;
    margin-left: 3px;
  }
  .banner .content-box .content .search-box {
    margin: 54px 0 0 94px;

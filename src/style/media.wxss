    height: 43px;
    border-radius: 21px;
  }
  .banner .content-box .content .search-box input {
    width: 586px;
    padding-left: 21px;
    margin-left: 4px;
    font-size: 14px;
  }
  .banner .content-box .content .search-box input:focus {
    outline: none;
  }
  .banner .content-box .content .search-box .btn {
    width: 57px;
    height: 43px;
    margin-left: 2px;
    border-radius: 0 21px 21px 0;
  }
  .banner .content-box .content .search-box .btn img {
    height: 14px;
    width: 14px;
  }
  .banner .content-box .content .banner-bottom {
    width: 852px;
    height: 27px;
    margin-top: 65px;

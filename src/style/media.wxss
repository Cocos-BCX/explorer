  }
  .body .content-box .count .num div span {
    font-size: 23px;
    line-height: 32px;
  }
  .body .content-box .count .num span {
    height: 14px;
    font-size: 14px;
    line-height: 15px;
  }
  .body .content-box .chart {
    width: 852px;
    margin: 17px auto 0;
  }
  .body .content-box .chart .child-chart {
    width: 419px;
    background: white;
  }
  .body .block_trade {
    width: 853px;
    margin: 17px auto 0;
    height: 532px;
  }
  .body .block_trade .title {
    height: 18px;
    font-size: 14px;
  }
  .body .block_trade .title span {
    font-size: 16px;
    color: #333;
  }
  .body .block_trade .title .more {
    font-size: 12px;
    margin-right: 18px;
  }
  .body .block_trade .block {
    width: 404px;
    padding: 23px 0 0 14px;
  }
  .body .block_trade .block .block-content {
    height: 511px;
  }
  .body .block_trade .block .block-piece {
    padding-top: 13px;
    height: 65px;
  }
  .body .block_trade .block .block-piece .block-id {
    height: 72px;
    width: 135px;
    font-size: 14px;
  }
  .body .block_trade .block .block-piece .block-detail {
    margin-left: 14px;
    font-size: 12px;
    height: 51px;
  }
  .body .block_trade .trade {
    margin-left: 12px;
  }
  .body .block_trade .trade .trade-info {
    font-size: 12px;
  }
  .body .block_trade .trade .trade-info span {
    margin-left: 4px;
  }
  .body .block_trade .trade .trade-info .trade-address {
    margin-top: 4px;
  }
  .body .block_trade .trade .trade-info .cut {
    height: 14px;
    width: 16px;
  }
  .body .block_trade .trade .trade-info .trade-id {
    margin-top: 4px;
  }
  .body .block_trade .trade .trade-info .trade-id span {
    width: 129px;
  }
  .body .block_trade .trade .trade-info .trade-num {
    margin-top: 4px;
  }
  .body .block_trade .trade .trade-time {
    font-size: 10px;
    margin-right: 15px;
  }
}

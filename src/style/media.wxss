@media (min-width: 1200px) and (max-width: 1440px) {
  .banner {
    height: 295px;
    width: 100%;
  }
  .banner .content-box {
    width: 1200px;
    height: 295px;
  }
  .banner .content-box .content {
    height: 295px;
    width: 1000px;
  }
  .banner .content-box .content .logo {
    margin-top: 25px;
    width: 110px;
    height: 48px;
  }
  .banner .content-box .content .logo img {
    width: 110px;
    height: 48px;
  }
  .banner .content-box .content .language {
    width: 75px;
    height: 23px;
    margin-top: 55px;
    border-radius: 3px;
    font-size: 10px;
  }
  .banner .content-box .content .language .icon {
    height: 9px;
    width: 12px;
    margin-left: 3px;
  }
  .banner .content-box .content .search-box {
    margin: 63px 0 0 110px;
    width: 781px;
    height: 50px;
    border-radius: 30px;
  }
  .banner .content-box .content .search-box input {
    width: 687px;
    padding-left: 30px;
    margin-left: 5px;
    font-size: 14px;
  }
  .banner .content-box .content .search-box input:focus {
    outline: none;
  }
  .banner .content-box .content .search-box .btn {
    width: 68px;
    height: 50px;
    margin-left: 2px;
    border-radius: 0 25px 25px 0;
    background: #388df4;
  }
  .banner .content-box .content .search-box .btn img {
    height: 16px;
    width: 16px;
  }
  .banner .content-box .content .banner-bottom {
    width: 1000px;
    height: 32px;
    margin-top: 77px;
    background: white;
  }
  .body {
    height: 900px;
    width: 100%;
  }
  .body .content-box {
    width: 1200px;
  }
  .body .content-box .count {
    display: flex;
    height: 94px;
    width: 1000px;
  }
  .body .content-box .count .num {
    width: 200px;
  }
  .body .content-box .count .num div {
    margin-top: 13px;
    font-size: 27px;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    color: #333333;
    line-height: 37px;
  }
  .body .content-box .count .num div span {
    font-size: 27px;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    color: #333333;
    line-height: 37px;
  }
  .body .content-box .count .num span {
    height: 17px;
    font-size: 15px;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #989898;
    line-height: 17px;
  }
  .body .content-box .chart {
    width: 1000px;
    margin: 20px auto 0;
  }
  .body .content-box .chart .child-chart {
    height: 342px;
    width: 492px;
  }
  .body .block_trade {
    width: 1000px;
    margin: 80px auto 0;
    height: 625px;
  }
  .body .block_trade .title {
    height: 26px;
    font-size: 18px;
  }
  .body .block_trade .title span {
    font-size: 17px;
  }
  .body .block_trade .title .more {
    font-size: 12px;
    font-family: PingFangSC-Regular;
    color: #388df4;
    margin-right: 18px;
    cursor: pointer;
  }
  .body .block_trade .block {
    width: 474px;
    padding: 27px 0 0 17px;
  }
  .body .block_trade .block .block-content {
    height: 600px;
  }
  .body .block_trade .block .block-piece {
    padding-top: 15px;
    height: 77px;
    border-bottom: 1px solid #e0e0e0;
    display: flex;
  }
  .body .block_trade .block .block-piece .block-id {
    height: 60px;
    width: 112px;
    font-size: 14px;
  }
  .body .block_trade .block .block-piece .block-detail {
    margin-left: 15px;
    font-size: 14px;
    height: 60px;
  }
  .body .block_trade .trade {
    margin-left: 18px;
  }
  .body .block_trade .trade .trade-info span {
    margin-left: 4px;
  }
  .body .block_trade .trade .trade-info .cut {
    height: 17px;
    width: 20px;
  }
  .body .block_trade .trade .trade-time {
    font-size: 14px;
    margin-right: 19px;
  }
}
@media (max-width: 1200px) {
